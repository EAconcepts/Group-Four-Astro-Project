---
import Layout from './Layout.astro';
import Header from '../components/Header/Header.astro';
import Footer from '../components/Footer/Footer.astro';
import Faq from '../components/Faq';

interface Props {
  title: string;
}

const { title } = Astro.props
---

<Layout title={title}>
  <Header />
  <slot />
	<Faq client:load/>
  <Footer />
</Layout>
